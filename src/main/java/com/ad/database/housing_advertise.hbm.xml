<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ad.database">
    <class name="com.ad.database.housing_advertise" table="housing_advertise">
        <id name="id" type="java.lang.Integer">
            <column name="id"></column>
            <generator class="identity"></generator>
        </id>
        <property name="hid" type="java.lang.Integer">
            <column name="hid"></column>
        </property>
        <property name="aid" type="java.lang.Integer">
            <column name="aid"></column>
        </property>
    </class>
</hibernate-mapping>